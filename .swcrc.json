{
	"jsc": {
		"baseUrl": ".",
		"externalHelpers": false,
		"keepClassNames": false,
		"loose": false,
		"parser": {
			"exportDefaultFrom": false,
			"exportNamespaceFrom": false,
			"decorators": false,
			"decoratorsBeforeExport": false,
			"dynamicImport": false,
			"functionBind": false,
			"importMeta": false,
			"jsx": false,
			"privateMethod": false,
			"syntax": "typescript",
			"topLevelAwait": false,
			"tsx": false
		},
		"paths": {
			"@enonic/js-utils": ["node_modules/@enonic/js-utils/src"],
			"enonic-types/*": ["node_modules/enonic-types/*"],
			"/OLDlib/explorer/*": ["src/main/resources/lib/explorer/*"],
			"/lib/explorer": ["build/rollup/index.js"]
		},
		"target": "es5",
		"transform": null
	},
	"module": {
		"type": "commonjs"
	},
	"sourceMaps": false
}
