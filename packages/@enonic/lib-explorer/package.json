{
  "author": "Christian Andreas Westgaard <cwe@enonic.com>",
  "browser": {
    "index.cjs": "browser.cjs",
    "index.mjs": "browser.mjs"
  },
  "bugs": {
    "url": "https://github.com/enonic/lib-explorer/issues"
  },
  "description": "constants and functions that can be used both client- and server-side.",
  "devDependencies": {
    "rimraf": "^3",
    "tsup": "^6"
  },
  "exports": {
    "types": "./index.d.ts",
    "import": "./index.mjs",
    "require": "./index.cjs"
  },
  "files": [
    "browser.cjs",
    "browser.mjs",
    "index.d.ts",
    "index.cjs",
    "index.mjs",
    "index.mts"
  ],
  "homepage": "https://github.com/enonic/lib-explorer/tree/master#readme",
  "keywords": [
    "commonjs",
    "enonic",
    "enonic-xp",
    "es5",
    "lib-explorer",
    "typescript"
  ],
  "license": "Apache-2.0",
  "main": "index.cjs",
  "module": "index.mjs",
  "name": "@enonic/lib-explorer",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enonic/lib-explorer"
  },
  "scripts": {
    "build": "yarn tsup:node:cjs:es5 && yarn tsup:node:esm && yarn tsup:browser",
    "clean": "rimraf browser.cjs browser.mjs index.cjs index.d.ts index.mjs",
    "prebuild": "yarn clean",
    "tsup:node:cjs:es5": "npx tsup --dts --format cjs --platform node --target es5 src/index.ts",
    "tsup:node:esm": "npx tsup --format esm --platform node src/index.ts",
    "tsup:browser": "npx tsup --entry.browser src/index.ts --format cjs,esm --platform browser"
  },
  "type": "module",
  "types": "index.d.mts",
  "version": "0.0.1"
}
